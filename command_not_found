#!/bin/bash

# link regex does not work because (http:)// gets recognized as file path
case $1 in
  fw)
    f w
    ;;
  o*)
    exists=${1:1}
    if [[ -e "$exists" ]]; then
      open "$exists"
    else
      echo "Did you mean: open $exists"
    fi
    ;;
  greo)
    shift
    grep --color=always "$@"
    ;;
  *)
    echo "Sorry, did not found: $*"
    echo "Maybe you can add it to handle if it was a frequent misspelling."
    ;;
esac
